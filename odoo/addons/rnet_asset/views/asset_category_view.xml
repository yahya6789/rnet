<odoo>
    <data>
        <record model="ir.ui.view" id="view_account_asset_category_form">
            <field name="name">View Account Asset Category Form</field>
            <field name="model">account.asset.category.custom</field>
            <field name="inherit_id" ref="odoo_account_asset.view_account_asset_category_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[1]/field[@name='type']" position="after">
                    <field name="asset_prefix"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
