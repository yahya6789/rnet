<odoo>
    <data>
        <record model="ir.ui.view" id="rnet_project_edit_project">
            <field name="name">rnet.project_edit_project</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <xpath expr="//page[1]/group[2]/group[1]/field[@name='no']" position="attributes">
                    <attribute name="string">Job Order No.</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>