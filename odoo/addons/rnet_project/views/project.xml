<odoo>
    <data noupdate="True">
        <record id="sequence_project" model="ir.sequence">
            <field name="name">Project No</field>
            <field name="code">project.no</field>
            <field name="active">TRUE</field>
            <field name="prefix">PROJ</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="rnet_project_view_project" model="ir.ui.view">
            <field name="name">rnet.project_view_project</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.view_project"/>
            <field name="arch" type="xml">
                <field name="name" position="before">
                    <field name="no"/>
                </field>
                <field name="partner_id" position="after">
                    <field name="type_id"/>
                    <field name="status_id"/>
                    <field name="contract_no"/>
                    <field name="contract_date"/>
                    <field name="reference_no"/>
                    <field name="plan_start_date"/>
                    <field name="plan_end_date"/>
                    <field name="actual_start_date"/>
                    <field name="actual_end_date"/>
                    <field name="description"/>
                </field>
            </field>
        </record>
    </data>
</odoo>